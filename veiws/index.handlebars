<div class="row">
  <center>

      <div class="col-md-4">
      <h3 class="submitFont">Krabby Patties Available</h3>
      <div id="pattyAvailable">

          {{#each patties}}
          {{#unless devoured}}

          <form action="patty/eat/{{id}}" method="POST">
            <div class="input-group">
              <li class="form-control">{{id}}. {{pattie_name}}</li>
                <span class="input-group-btn">
                  <button class="btn btn-success devour-button" type="submit">Devour It!</button>
                </span>
            </div>
          </form>
          {{/unless}}
        {{/each}}

      </div>
    </div>

    <div class="col-md-4"></div> 

    <div class="col-md-4">
        <h3 class="submitFont">Krabby Patties Devoured</h3>"
        <div id="alresdyDevoured">

            {{#each patties}}
            {#if this.devoured}}
            <div class="input-group">
              <li class="form-control">{{this.id}}. {{this.pattie_name}}</li>
              </div>
              {{/if}}
              {{each}}

              </div>
              </div>
  </center>
  </div>
  